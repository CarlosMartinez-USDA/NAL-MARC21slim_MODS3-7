   <mods version="3.7">
      <titleInfo>
         <title>Natural regeneration in a beech-dominated forest managed by close-to-nature principles --- a gap cutting based experiment</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">K.</namePart>
         <namePart type="family">Hahn</namePart>
         <displayForm>Hahn, K.</displayForm>
      </name>
      <name type="personal">
         <namePart type="given">P.</namePart>
         <namePart type="family">Madsen</namePart>
         <displayForm>Madsen, P.</displayForm>
      </name>
      <typeOfResource>text</typeOfResource>
      <originInfo>
         <dateIssued encoding="w3cdtf" keyDate="yes">2008-07</dateIssued>
