    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Woody plant encroachment amplifies spatial heterogeneity of soil phosphorus
        to considerable depth</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 136-147.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
      <subfield code="a">JOURNAL ARTICLE</subfield>
    </datafield>
    <datafield tag="506" ind1=" " ind2=" ">
