    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Temperature dependence, spatial scale, and tree species diversity in
        eastern Asia and North America</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 13388-13392.</subfield>
