    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Leaf- and crown-level adjustments help giant sequoias maintain favorable
        water status during severe drought</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 257-267.</subfield>
    </datafield>
    <datafield tag="506" ind1=" " ind2=" ">
