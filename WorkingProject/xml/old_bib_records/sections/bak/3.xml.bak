    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Thinning in mature eastern white pine: 43-year case study</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 539-549.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
