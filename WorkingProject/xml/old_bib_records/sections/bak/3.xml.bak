    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Diversity of Bacterial Communities in Container Habitats of
        Mosquitoes</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 593-603.</subfield>
    </datafield>
    <datafield tag="506" ind1=" " ind2=" ">
