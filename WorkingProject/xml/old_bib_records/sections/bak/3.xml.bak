    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Canopy microclimate response to pattern and density of thinning in a Sierra
        Nevada forest</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 435-442.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
