    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Soil water depletion by Eucalyptus spp. integrated into dryland
        agricultural systems</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 141-151.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
