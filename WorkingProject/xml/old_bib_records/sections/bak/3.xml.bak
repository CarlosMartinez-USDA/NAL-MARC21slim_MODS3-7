    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Isotope composition and anion chemistry of soil profiles along the Kalahari
        Transect</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 480-486.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
