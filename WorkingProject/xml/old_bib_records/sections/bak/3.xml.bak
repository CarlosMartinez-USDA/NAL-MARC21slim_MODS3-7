    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">High-spatiotemporal resolution mapping of spatiotemporally continuous
        atmospheric CO2 concentrations over the global continent</subfield>
    </datafield>
    <datafield tag="506" ind1=" " ind2=" ">
      <subfield code="a">Creative Commons Attribution 4.0 Generic (CC BY 4.0)</subfield>
      <subfield code="u">http://creativecommons.org/licenses/by/4.0/</subfield>
      <subfield code="f">Unrestricted online access</subfield>
