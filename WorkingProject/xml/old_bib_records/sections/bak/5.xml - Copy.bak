    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Tree‐to‐tree interactions slow down Himalayan treeline shifts as inferred
        from tree spatial patterns</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 1816-1826.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
