    <datafield tag="100" ind1="1" ind2=" ">
      <subfield code="a">Rezende, Vanessa Leite</subfield>
      <subfield code="e">author</subfield>
      <subfield code="u">Departamento de Biologia, Setor de Ecologia e Conservação, Universidade
