    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Large fires as agents of ecological diversity in the North American boreal
        forest</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 754-767.</subfield>
