    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Neighborhood competition and crown asymmetry in Acer saccharum</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 2151-2159.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
