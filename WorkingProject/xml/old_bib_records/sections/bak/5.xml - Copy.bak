    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Conspecific negative density‐dependent mortality and the structure of
        temperate forests</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 2493-2503.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
      <subfield code="a">JOURNAL ARTICLE</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
