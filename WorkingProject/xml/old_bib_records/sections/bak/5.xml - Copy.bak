    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Gap disturbance regime in an old-growth Fagus-Abies forest in the Dinaric
        Mountains, Bosnia-Herzegovina</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 2728-2737.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
