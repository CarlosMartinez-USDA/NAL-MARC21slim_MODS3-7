    <datafield tag="100" ind1="1" ind2=" ">
      <subfield code="a">Howard, Ian M.</subfield>
      <subfield code="e">author</subfield>
      <subfield code="u">Department of Geosciences, University of Arkansas, Fayetteville, AR, USA.
        ihowardksu@gmail.com</subfield>
