    <datafield tag="100" ind1="1" ind2=" ">
      <subfield code="a">Yan, En-Rong</subfield>
      <subfield code="e">author</subfield>
      <subfield code="u">Forest Ecosystem Research and Observation Station in Putuo Island, School
        of Ecological and Environmental Sciences, East China Normal University, Shanghai, 200241,
        China. eryan@des.ecnu.edu.cn</subfield>
      <subfield code="1">http://orcid.org/0000-0002-8064-3334</subfield>
