    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Assessing long-term hydrological and ecological responses to drainage in a
        raised bog using paleoecology and a hydrosequence</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 143-156.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
