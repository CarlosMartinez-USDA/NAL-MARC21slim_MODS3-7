    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Identifying roots of northern hardwood species: patterns with diameter and
        depth</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 2862-2869.</subfield>
