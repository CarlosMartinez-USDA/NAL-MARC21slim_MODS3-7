    <datafield tag="100" ind1="1" ind2=" ">
      <subfield code="a">Camarero, Jesús Julio</subfield>
      <subfield code="e">author</subfield>
      <subfield code="u">ARAID, Instituto Pirenaico de Ecología, Consejo Superior de Investigaciones
