    <datafield tag="100" ind1="1" ind2=" ">
      <subfield code="a">Pommerening, Arne</subfield>
      <subfield code="e">author</subfield>
      <subfield code="u">Swedish University of Agricultural Sciences SLU, Faculty of Forest
        Sciences, Department of Forest Ecology and Management, Skogsmarksgränd 17, SE-901 83 Umeå,
        Sweden</subfield>
      <subfield code="1">https://orcid.org/0000-0002-3441-2391</subfield>
