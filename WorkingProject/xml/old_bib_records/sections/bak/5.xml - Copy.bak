    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Context-dependent recruitment of Guettarda viburnoides in a Neotropical
        forest-savanna mosaic</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 1317-1326.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
