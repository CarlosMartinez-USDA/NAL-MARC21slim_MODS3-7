    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Tree spatial patterns and stand development along the pine-grassland
        ecotone in the Colorado Front Range</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 575-584.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
