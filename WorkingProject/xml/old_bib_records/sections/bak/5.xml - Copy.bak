    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Assessment of assisted migration effects on spring bud flush in white
        spruce (Picea glauca [Moench] Voss) seedlings</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 391-397.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
