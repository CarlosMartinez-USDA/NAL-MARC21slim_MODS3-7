    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Post-fire tree establishment patterns at the alpine treeline ecotone: Mount
        Rainier National Park, Washington, USA</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 107-120.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
