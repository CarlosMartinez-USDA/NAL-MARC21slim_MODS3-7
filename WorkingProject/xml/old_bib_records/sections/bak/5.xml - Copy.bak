    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Ontogenetic trait variation and metacommunity effects influence species
        relative abundances during tree community assembly</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 360-368.</subfield>
    </datafield>
    <datafield tag="506" ind1=" " ind2=" ">
      <subfield code="a">Creative Commons Attribution 4.0 Generic (CC BY-NC-ND 4.0)</subfield>
