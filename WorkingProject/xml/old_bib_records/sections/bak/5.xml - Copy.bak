    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Fine-scale species co-occurrence patterns in an old-growth temperate
        forest</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
      <subfield code="a">p. 2115-2120.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
