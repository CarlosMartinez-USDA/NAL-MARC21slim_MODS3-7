      <titleInfo>
         <title>Integrative analysis of polychaete ontogeny: cell proliferation patterns         and myogenesis in trochophore larvae of Sabellaria alveolata</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Nora</namePart>
         <namePart type="family">Brinkmann</namePart>
         <displayForm>Brinkmann, Nora</displayForm>
      </name>
      <name type="personal">
         <namePart type="given">Andreas</namePart>
         <namePart type="family">Wanninger</namePart>
         <displayForm>Wanninger, Andreas</displayForm>
      </name>
      <typeOfResource>text</typeOfResource>
      <originInfo>
         <dateIssued encoding="w3cdtf" keyDate="yes">2010-01</dateIssued>
