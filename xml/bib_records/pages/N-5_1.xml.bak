      <titleInfo>
         <title>Optimizing Forest Spatial Structure with Neighborhood-Based Indices: Four         Case Studies from Northeast China</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Lingbo</namePart>
         <namePart type="family">Dong</namePart>
