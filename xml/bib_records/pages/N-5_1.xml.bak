         <namePart type="given">T.</namePart>
         <namePart type="family">Kuuluvainen</namePart>
         <displayForm>Kuuluvainen, T.</displayForm>
         <affiliation>University of Helsinki, Department of Forest Ecology P. O. Box 27, FI-00014         University of Helsinki, Finland</affiliation>
         <role>
            <roleTerm type="text">author</roleTerm>
         </role>
      </name>
