      <titleInfo>
         <title>Assessment of assisted migration effects on spring bud flush in white         spruce (Picea glauca [Moench] Voss) seedlings</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Pengxin</namePart>
         <namePart type="family">Lu</namePart>
         <displayForm>Lu, Pengxin</displayForm>
      </name>
