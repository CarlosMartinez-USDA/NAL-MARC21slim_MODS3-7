      <titleInfo>
         <title>Spatial patterns of wood traits in China are controlled by phylogeny and         the environment</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Shi-Bao</namePart>
         <namePart type="family">Zhang</namePart>
         <displayForm>Zhang, Shi-Bao</displayForm>
      </name>
