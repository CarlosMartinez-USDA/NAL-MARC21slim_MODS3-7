      <titleInfo>
         <title>Factors influencing male mating success in bur oak, Quercus         macrocarpa</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">B.D.</namePart>
         <namePart type="family">Dow</namePart>
         <displayForm>Dow, B.D.</displayForm>
      </name>
