      <titleInfo>
         <title>Dynamics of soil respiration in sparse Ulmus pumila woodland under         semi-arid climate</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Hong-Mei</namePart>
         <namePart type="family">Jin</namePart>
