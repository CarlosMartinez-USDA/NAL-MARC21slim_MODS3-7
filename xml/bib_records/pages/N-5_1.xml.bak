      <titleInfo>
         <title>Fine‐scale tree spatial patterns are shaped by dispersal limitation which         correlates with functional traits in a natural temperate forest</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Roxane</namePart>
         <namePart type="family">Beyns</namePart>
