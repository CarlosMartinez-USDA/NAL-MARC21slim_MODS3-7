      <titleInfo>
         <title>Pollen selection by feral honey bee (Hymenoptera: Apidae) colonies in a         coastal prairie landscape</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">K.A.</namePart>
         <namePart type="family">Baum</namePart>
         <displayForm>Baum, K.A.</displayForm>
