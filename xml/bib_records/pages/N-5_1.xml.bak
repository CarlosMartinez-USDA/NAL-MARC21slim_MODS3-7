      <titleInfo>
         <title>Spatial Dynamics of Natural Regeneration in Two Differently Managed Pinus         sylvestris Stands before and after Silvicultural Intervention Using Replicated Spatial Point         Patterns</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Marta</namePart>
         <namePart type="family">Pardos</namePart>
