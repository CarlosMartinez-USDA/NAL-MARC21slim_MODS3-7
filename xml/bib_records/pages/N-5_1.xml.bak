      <titleInfo>
         <title>Causes of Decline in the Korean Fir Based on Spatial Distribution in the         Mt. Halla Region in Korea: A Meta-Analysis</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Ung San</namePart>
         <namePart type="family">Ahn</namePart>
         <displayForm>Ahn, Ung San</displayForm>
         <affiliation>Research Department for Hallasan, World Heritage Office, 72 Sumokwon-gil,         Jeju-si, Jeju Special Self-Governing Province, Jeju 63143, Korea</affiliation>
