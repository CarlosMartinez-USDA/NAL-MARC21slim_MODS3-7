      <titleInfo>
         <nonSort>The </nonSort>
         <title>relevance of ants as seed rescuers of a primarily bird-dispersed tree         in the Neotropical cerrado savanna</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Alexander V.</namePart>
         <namePart type="family">Christianini</namePart>
         <displayForm>Christianini, Alexander V.</displayForm>
      </name>
      <name type="personal">
         <namePart type="given">Paulo S.</namePart>
         <namePart type="family">Oliveira</namePart>
         <displayForm>Oliveira, Paulo S.</displayForm>
      </name>
