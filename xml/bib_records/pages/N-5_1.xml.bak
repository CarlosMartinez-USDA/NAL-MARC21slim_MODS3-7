      <titleInfo>
         <title>Distribution and dynamics of two ferns Dennstaedtia punctilobula         (Dennstaedtiaceae) and Thelypteris noveboracensis (Thelypteridaceae) in a northeast mixed         hardwoods-hemlock forest</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">J.D.</namePart>
         <namePart type="family">Hill</namePart>
         <displayForm>Hill, J.D.</displayForm>
      </name>
      <name type="personal">
