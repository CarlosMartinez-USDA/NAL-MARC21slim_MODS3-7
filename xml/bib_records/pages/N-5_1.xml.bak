      <titleInfo>
         <title>Biomass Allocation and Leaf Morphology of Saplings Grown under Various         Conditions of Light Availability and Competition Types</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Ieva</namePart>
