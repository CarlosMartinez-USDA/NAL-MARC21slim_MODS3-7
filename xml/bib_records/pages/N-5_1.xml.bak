      <titleInfo>
         <title>Origin of spatial genetic structure in an expanding oak         population</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Arndt</namePart>
         <namePart type="family">Hampe</namePart>
         <displayForm>Hampe, Arndt</displayForm>
      </name>
      <name type="personal">
         <namePart type="given">Leila</namePart>
         <namePart type="family">El Masri</namePart>
         <displayForm>El Masri, Leila</displayForm>
      </name>
