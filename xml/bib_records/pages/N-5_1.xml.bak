      <titleInfo>
         <title>Spatial patterns of trees of different size in boreal &lt;i&gt;Pinus         sylvestris &lt;/i&gt;forest sites with different fire histories</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">S.</namePart>
         <namePart type="family">Rouvinen</namePart>
         <displayForm>Rouvinen, S.</displayForm>
         <affiliation>University of Joensuu, Faculty of Forestry P. O. Box 111, FI-80101 Joensuu,         Finland</affiliation>
