      <titleInfo>
         <title>Neighborhood competition and crown asymmetry in Acer saccharum</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">J.</namePart>
         <namePart type="family">Brisson</namePart>
         <displayForm>Brisson, J.</displayForm>
      </name>
      <typeOfResource>text</typeOfResource>
