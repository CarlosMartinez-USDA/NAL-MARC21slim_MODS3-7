      <titleInfo>
         <title>Distribution of forest ecosystems over two centuries in a highly urbanized         landscape</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Robert T.</namePart>
         <namePart type="family">Fahey</namePart>
         <displayForm>Fahey, Robert T.</displayForm>
         <affiliation>Department of Natural Resources and the Environment &amp; Center for         Environmental Sciences and Engineering, University of Connecticut, 1376 Storrs Rd. Unit         4087, Storrs, CT 06269, USA</affiliation>
