      <titleInfo>
         <title>Using potential distributions to explore determinants of Western                 Palaearctic migratory songbird species richness in sub-Saharan Africa</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">M.S.</namePart>
         <namePart type="family">Wisz</namePart>
         <displayForm>Wisz, M.S.</displayForm>
      </name>
