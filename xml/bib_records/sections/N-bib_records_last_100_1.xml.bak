      <titleInfo>
         <title>Patchy Fires Promote Regeneration of Longleaf Pine (&lt;i&gt;Pinus                 palustris&lt;/i&gt; Mill.) in Pine Savannas</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Kevin M.</namePart>
         <namePart type="family">Robertson</namePart>
         <displayForm>Robertson, Kevin M.</displayForm>
         <affiliation>Tall Timbers Research Station, Tallahassee, FL 32312, USA.                 krobertson@talltimbers.org</affiliation>
