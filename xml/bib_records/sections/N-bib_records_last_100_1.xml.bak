      <titleInfo>
         <title>Spatial and temporal patterns of throughfall chemistry within a                 temperate mixed oak-beech stand</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Frédéric</namePart>
         <namePart type="family">André</namePart>
