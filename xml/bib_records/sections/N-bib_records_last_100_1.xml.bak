      <titleInfo>
         <title>Biases in studies of spatial patterns in insect herbivory</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Elena L.</namePart>
         <namePart type="family">Zvereva</namePart>
         <displayForm>Zvereva, Elena L.</displayForm>
