      <titleInfo>
         <title>Patterns from the past: modeling Public Land Survey witness tree                 distributions with weights-of-evidence</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Todd D.</namePart>
         <namePart type="family">Fagin</namePart>
         <displayForm>Fagin, Todd D.</displayForm>
      </name>
