      <titleInfo>
         <title>Learning to fly: integrating spatial ecology with unmanned aerial                 vehicle surveys</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Peter W. J.</namePart>
         <namePart type="family">Baxter</namePart>
         <displayForm>Baxter, Peter W. J.</displayForm>
