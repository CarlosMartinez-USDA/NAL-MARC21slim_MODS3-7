      <titleInfo>
         <title>Biogeographic patterns in Mediterranean and Macaronesian species of                 Saxifraga (Saxifragaceae) inferred from phylogenetic analyses of ITS                 sequences</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">P.</namePart>
         <namePart type="family">Vargas</namePart>
         <displayForm>Vargas, P.</displayForm>
      </name>
      <name type="personal">
