      <titleInfo>
         <title>Modeling spatio-temporal crop allocation patterns by a stochastic                 decision tree method, considering agronomic driving factors</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Luc</namePart>
