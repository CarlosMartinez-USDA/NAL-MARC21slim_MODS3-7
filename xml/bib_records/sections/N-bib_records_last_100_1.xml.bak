      <titleInfo>
         <title>Comparison of spatial patterns of soil seed banks between larch                 plantations and adjacent secondary forests in Northeast China: implication for                 spatial distribution of larch plantations</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Qiaoling</namePart>
         <namePart type="family">Yan</namePart>
         <displayForm>Yan, Qiaoling</displayForm>
         <affiliation>State Key Laboratory of Forest and Soil Ecology, Institute of Applied                 Ecology, Chinese Academy of Sciences, Shenyang, 110164, China,</affiliation>
