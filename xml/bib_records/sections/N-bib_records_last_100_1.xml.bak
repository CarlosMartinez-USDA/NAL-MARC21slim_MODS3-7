      <titleInfo>
         <title>Modeling the Effects of Fire Severity and Spatial Complexity on Small                 Mammals in Yosemite National Park, California</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Susan L.</namePart>
         <namePart type="family">Roberts</namePart>
         <displayForm>Roberts, Susan L.</displayForm>
