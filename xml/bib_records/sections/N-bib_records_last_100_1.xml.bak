      <titleInfo>
         <title>Disturbance‐driven variation in abundance–occupancy relationships                 over time in tropical forest fragments</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Collins Edward</namePart>
         <namePart type="family">Bulafu</namePart>
         <displayForm>Bulafu, Collins Edward</displayForm>
         <affiliation>Department of Biological Sciences, School of Bio‐Sciences, College of                 Natural Sciences, Makerere University, PO Box 7062, Kampala, Uganda</affiliation>
         <role>
