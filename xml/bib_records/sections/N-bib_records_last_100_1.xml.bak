      <titleInfo>
         <title>Differences in the Spatial Structure of Two &lt;i&gt;Pinus                 cembra&lt;/i&gt; L. Populations in the Carpathian Mountains</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Mirela</namePart>
         <namePart type="family">Beloiu</namePart>
         <displayForm>Beloiu, Mirela </displayForm>
         <affiliation>Department of Biogeography, University of Bayreuth,                 Universitätsstraße 30, 95447 Bayreuth, Germany.                 mirela.beloiu@uni-bayreuth.de</affiliation>
         <role>
            <roleTerm type="text">author</roleTerm>
         </role>
      </name>
