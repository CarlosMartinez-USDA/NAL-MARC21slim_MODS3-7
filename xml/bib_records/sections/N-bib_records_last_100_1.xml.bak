      <titleInfo>
         <title>Spatial pattern of brown rot within peach trees related to inoculum                 of Monilinia fructicola in organic orchard</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">C.</namePart>
         <namePart type="family">Keske</namePart>
         <displayForm>Keske, C.</displayForm>
      </name>
