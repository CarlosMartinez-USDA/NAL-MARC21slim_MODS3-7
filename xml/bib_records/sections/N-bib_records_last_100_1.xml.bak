      <titleInfo>
         <title>Multiple attribute decision making for individual tree detection                 using high-resolution laser scanning</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Giovanni</namePart>
         <namePart type="family">Forzieri</namePart>
         <displayForm>Forzieri, Giovanni</displayForm>
      </name>
      <name type="personal">
