      <titleInfo>
         <title>In trangenic rice, α- and β-tubulin regulatory sequences control GUS                 amount and distribution through intron mediated enhancement and intron dependent                 spatial expression</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Silvia</namePart>
